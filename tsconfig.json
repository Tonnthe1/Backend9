{
  "compilerOptions": {
    "target": "es6",
    "types": ["node", "express", "cors", "jsonwebtoken", "bcryptjs"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}